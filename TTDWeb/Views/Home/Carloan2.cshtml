@model  TTDWeb.Models.CarLoanStep2

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Carloan2</title>
    <link rel="stylesheet" type="text/css" href="~/content/css/css.css">
<style type="text/css">
    body
    {
        background-image: none;
        background-color: #FFF;
        color: #282828;
    }
</style>
</head>
<body>
<div style="width:650px; text-align:center; margin-top:30px; margin-bottom:30px;"><img src="../content/images/apply21.png" width="573" height="44" /></div>

@using (Html.BeginForm("Carloan3", "Home", FormMethod.Post))
{
<div style=" background-color:#f7f7f7; padding-left:70px; padding-top:35px; height:600px;">
    <div style="width:340px; margin-left:85px;"><span style="color:#5489d2; font-size:30px;">天天贷郑重承诺</span><br />
天天贷致力保护您的个人隐私，您提供的信息将不会以任何形式公布，
有任何疑问，请致电天天贷网客服：400-8055-855</div>
<br />
<div class="inputtitle">您的姓名：</div>@Html.TextBoxFor(m=>m.CustomerName,new { placeholder="如：王先生",@class="inputtext"})<br /><br />
<div class="inputtitle">手机号码：</div>@Html.TextBoxFor(m=>m.CustomerPhone,new { placeholder="用于接收信贷员联系方式",@class="inputtext"})<br /><br />
<div class="inputtitle">电子邮箱：</div>@Html.TextBoxFor(m=>m.CustomerEmail,new { placeholder="用于接收申请贷款所需材料",@class="inputtext"})<br /><br />
<div class="inputtitle">&nbsp;</div><button type="submit" class="btn" style="font-size:14px; height:35px; width:110px;">下一步</button>
        </div>
}

</body>
</html>
